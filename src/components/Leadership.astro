---
import CVEntry from '../components/CVEntry.astro'
import leadershipEntries from '../../public/data/leadership.json'
---
<!-- Esto es un for para colocar voluntairado -->
 
<!-- Si quiero cambiar mi contenido ir a json en public, si quiero cambiar el formato ir Cventry -->
<ol class={`relative border-s dark:border-orange ${Astro.props}`}>                  
    {leadershipEntries.map((entry) => (
        <CVEntry 
            position={entry.position}
            institution={entry.institution}
            location={entry.location}
            startDate=''
            endDate=''
            year={entry.year}
            description={entry.description}
        />
    ))}
</ol>
